{
    "version": 2,
    "public": true,
    "builds": [
      { "src": "api/bot.js", "use": "@vercel/node" },
      { "src": "api/_middleware.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/bot",
        "dest": "/api/bot.js",
        "methods": ["POST"]
      },
      {
        "src": "/",
        "dest": "/api/_middleware.js"
      }
    ]
  }